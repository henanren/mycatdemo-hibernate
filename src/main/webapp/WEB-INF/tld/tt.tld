<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE taglib PUBLIC "-//Sun Microsystems, Inc.//DTD JSP Tag Library 1.2//EN"
                        "http://java.sun.com/dtd/web-jsptaglibrary_1_2.dtd">
<taglib>
	<tlib-version>1.0</tlib-version>
	<jsp-version>1.2</jsp-version>
	<short-name>tt</short-name>
	<uri>/tt-tags</uri>
	<display-name>自定义标签</display-name>


	<!-- =========自动引入JS CSS -->
	<tag>
		<name>head</name>
		<tag-class>
			persistent.prestige.platform.taglib.HeadTag
		</tag-class>
		<body-content>empty</body-content>
		<description>自动引入JS</description>
		<attribute>
			<name>theme</name>
			<required>false</required>
			<rtexprvalue>false</rtexprvalue>
			<description>
				风格,目前支持bootstrap,
				default,metro，如果自定义(component/easyui/themes/)
			</description>
		</attribute>
		<attribute>
			<name>resTypes</name>
			<required>false</required>
			<rtexprvalue>false</rtexprvalue>
			<description>
				定义需要引入的JS/CSS类别;默认为all,可选值
				jquery,jquery_cookie,jquery_form,jquery_json,jquery_url,jquery_easy_ui,jquery_validate,tt-all,
				tt_base,tt_base_ui,tt_form,tt_grid,tt_tree,tt_tab,tt_window,tt_date,tt_rds,all
			</description>
		</attribute>
	</tag>

	<!-- ============== rds Tag ====================== -->
	<tag>
		<name>rds</name>
		<tag-class>
			persistent.prestige.platform.rds.taglib.RdsTag
		</tag-class>
		<body-content>empty</body-content>
		<description>rds. 数据获取控件</description>
		<attribute>
			<name>name</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
			<description>name. rds js Object</description>
		</attribute>
		<attribute>
			<name>dataProvider</name>
			<required>true</required>
			<rtexprvalue>false</rtexprvalue>
			<description>
				dataProvider. 数据提供者，可以是Model实体类，也可以是受Spring管理的Service
				Bean Id
			</description>
		</attribute>
		<attribute>
			<name>conditions</name>
			<rtexprvalue>true</rtexprvalue>
			<description>
				conditions.查询条件
				conditions="qf_name:dfd,qf_ps:1",其中qf_表示查询,qp_查询模式,qs_排序模式
			</description>
		</attribute>
		<attribute>
			<name>query</name>
			<rtexprvalue>false</rtexprvalue>
			<description>
				query.查询接口，可以是全类名，亦可以是受Spring 管理的 Bean ID
			</description>
		</attribute>
		<attribute>
			<name>extrProps</name>
			<rtexprvalue>true</rtexprvalue>
			<description>导出实体属性,默认为*,代表导出实体普通属性</description>
		</attribute>
	</tag>

	<!-- =========将实体对象放入pageContext范围对象中 -->
	<tag>
		<name>entityGet</name>
		<tag-class>
			persistent.prestige.platform.rds.taglib.RdsEntityGetTag
		</tag-class>
		<body-content>empty</body-content>
		<description>将实体对象放入pageContext范围对象中</description>
		<attribute>
			<name>var</name>
			<required>true</required>
			<rtexprvalue>false</rtexprvalue>
			<description>范围对象KEY</description>
		</attribute>
		<attribute>
			<name>modelClass</name>
			<required>true</required>
			<rtexprvalue>false</rtexprvalue>
			<description>实体全路径名</description>
		</attribute>
		<attribute>
			<name>moId</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
			<description>实体ID</description>
		</attribute>
		<attribute>
			<name>extrProps</name>
			<required>false</required>
			<rtexprvalue>false</rtexprvalue>
			<description>导出的属性，默认为所有基本属性</description>
		</attribute>
	</tag>

